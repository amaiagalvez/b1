{
    "name": "izt/users",
    "description": "Basics Management",
    "version": "1.0.0",
    "authors": [
        {
            "name": "IZT",
            "email": "amaia@izt.coop"
        }
    ],
    "require": {
        "php": "^7.2",
        "league/fractal": "^0.19.2",
        "yajra/laravel-datatables": "^1.5",
        "yajra/laravel-datatables-fractal": "^1.6",
        "laravel/socialite": "^4.4",
        "izt/helpers": "^2.0"
    },
    "require-dev": {
        "orchestra/testbench": "^3.8",
        "phpunit/phpunit": "^8.0",
        "laravel/dusk": "^6.4"
    },
    "autoload": {
        "psr-4": {
            "Izt\\Users\\" : "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Izt\\Users\\Tests\\" : "tests/"
        },
        "files": [
            "database/seeds/VariablesTableSeeder.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Izt\\Users\\UsersServiceProvider",
                "Izt\\Users\\RouteServiceProvider"
            ],
            "aliases": {

            }
        }
    },
    "repositories": [
        {
            "type": "path",
            "url": "../Helpers"
        }
    ]
}
