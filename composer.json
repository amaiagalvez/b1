{
    "name": "izt/basics",
    "description": "Basics Management",
    "version": "1.0.0",
    "authors": [
        {
            "name": "IZT",
            "email": "amaia@izt.coop"
        }
    ],
    "require": {
        "php": "^7.2",
        "yajra/laravel-datatables": "^1.5",
        "laravel/socialite": "^4.4",
        "izt/helpers": "^1.0"
    },
    "require-dev": {
        "orchestra/testbench": "^3.8"
    },
    "autoload": {
        "psr-4": {
            "Izt\\Basics\\" : "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Izt\\Basics\\Tests\\" : "tests/"
        },
        "files": [
            "database/seeds/VariablesTableSeeder.php",
            "database/seeds/RolesTableSeeder.php",
            "database/seeds/UsersTableSeeder.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Izt\\Basics\\BasicsServiceProvider",
                "Izt\\Basics\\RouteServiceProvider"
            ],
            "aliases": {

            }
        }
    },
    "repositories": [
        {
            "type": "path",
            "url": "../Helpers"
        }
    ]
}
